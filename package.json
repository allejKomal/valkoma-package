{
  "name": "valkoma-package",
  "version": "0.1.9",
  "description": "A React TypeScript component library with shadcn/ui components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src/index.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./primitive": {
      "import": "./dist/primitive.esm.js",
      "require": "./dist/primitive.cjs.js",
      "types": "./dist/primitive.d.ts"
    },
    "./design-system": {
      "import": "./dist/design-system.esm.js",
      "require": "./dist/design-system.cjs.js",
      "types": "./dist/design-system.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.esm.js",
      "require": "./dist/hooks.cjs.js",
      "types": "./dist/hooks.d.ts"
    },
    "./lib": {
      "import": "./dist/lib.esm.js",
      "require": "./dist/lib.cjs.js",
      "types": "./dist/lib.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css"
    }
  },
  "typesVersions": {
    "*": {
      "primitive": [
        "dist/primitive.d.ts"
      ],
      "design-system": [
        "dist/design-system.d.ts"
      ],
      "hooks": [
        "dist/hooks.d.ts"
      ],
      "lib": [
        "dist/lib.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "shadcn-ui",
    "component-library"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "postcss": "^8.5.6",
    "prettier": "^2.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^2.0.0",
    "vitest": "^0.34.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.263.1",
    "react-day-picker": "^9.8.0",
    "react-hook-form": "^7.61.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2"
  }
}
